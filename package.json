{
  "name": "gowa-manager",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:tsc": "tsc --noEmit",
    "dev:server": "bun --watch run src/index.ts",
    "dev:client": "cd client && bun run dev",
    "dev": "cd client && bun run build:watch & bun --watch run src/index.ts",
    "build:client": "cd client && bun run build",
    "test": "bun run test-server.ts",
    "test:isolation": "bun run test-isolation.ts",
    "reset-db": "bun run reset-db.ts",
    "compile": "bun build --compile --minify --target bun --outfile gowa-manager --bytecode ./src/index.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/static": "^1.3.0",
    "elysia": "^1.3.21",
    "pidusage": "^4.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bun": "latest",
    "typescript": "^5.9.2"
  }
}
